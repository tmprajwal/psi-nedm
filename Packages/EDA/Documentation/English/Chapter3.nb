 CellTags->{"Precision--determination", "Standard deviation--determination"}],

Cell[BoxData[
 RowBox[{
  SubscriptBox["\[Sigma]", "est"], " ", "=", 
  SqrtBox[
   FractionBox[
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "1"}], "n"], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        RowBox[{"x", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
        "-", " ", 
        OverscriptBox["x", "_"]}], ")"}], "2"]}], 
    RowBox[{"n", "-", "1"}]]]}]], "DisplayFormula",
 Evaluatable->False,
 AspectRatioFixed->True,
 CellTags->{"Precision--determination", "Standard deviation--determination"}],

Cell[TextData[{
 "The major difference between this estimate and the definition is the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"n", " ", "-", " ", "1"}], ")"}], TraditionalForm]]],
 " in the denominator instead of ",
 StyleBox["n",
  FontSlant->"Italic"],
 ". This is reasonable since if ",
 StyleBox["n",
  FontSlant->"Italic"],
 " = 1 we know we can",
 "\[CloseCurlyQuote]",
 "t determine \[Sigma] at all since with only one measurement we have no way \
of determining how closely a repeated measurement might give the same result. \
Technically, the quantity ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"n", " ", "-", " ", "1"}], ")"}], TraditionalForm]]],
 " is the \[OpenCurlyDoubleQuote]number of degrees of freedom\
\[CloseCurlyDoubleQuote] of the sample of measurements."
}], "Text",
 Evaluatable->False,
 AspectRatioFixed->True,
 CellTags->{"Precision--determination", "Standard deviation--determination"}],


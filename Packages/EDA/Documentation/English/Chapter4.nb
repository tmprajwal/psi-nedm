Cell[TextData[{
 "To a good approximation, the uncertainty in ",
 StyleBox["y",
  FontSlant->"Italic"],
 ", because of the errors in ",
 StyleBox["x", "TR",
  FontSlant->"Italic"],
 StyleBox[",",
  FontSlant->"Italic"],
 " is the error in ",
 StyleBox["x",
  FontSlant->"Italic"],
 " times the slope of the line."
}], "Text",
 Evaluatable->False,
 AspectRatioFixed->True,
 CellTags->{
  "Fitting--experimental errors in both coordinates", 
   "Effective variance algorithm"}],

Cell[BoxData[
 RowBox[{
  RowBox[{

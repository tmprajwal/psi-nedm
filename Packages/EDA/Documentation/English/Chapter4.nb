Cell["\<\
The central idea of the Brent algorithm is that we weight the sum of the \
squares of the residuals with the effective variance errors.\
\>", "Text",
 Evaluatable->False,
 AspectRatioFixed->True,
 CellTags->{"LinearFit--Brent option", "Brent option to LinearFit"}],

Cell[BoxData[
 RowBox[{
  UnderoverscriptBox["\[Sum]", 
   RowBox[{"i", "=", "1"}], "N"], 
  RowBox[{
   FractionBox["1", 
    SuperscriptBox[
     RowBox[{"errvar", "[", 
      RowBox[{"[", "i", "]"}], "]"}], "2"]], 
   RowBox[{"(", " ", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"y", "[", 
        RowBox[{"[", "i", "]"}], "]"}], " ", "-", " ", 
       RowBox[{
        RowBox[{"a", "[", "0", "]"}], " ", 
        RowBox[{"X", "[", 
         RowBox[{
          RowBox[{"x", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ",", " ", "0"}], "]"}]}], " ", "-",
        " ", 
       RowBox[{
        RowBox[{"a", "[", "1", "]"}], 
        RowBox[{"X", "[", 
         RowBox[{
          RowBox[{"x", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ",", " ", "1"}], "]"}]}], " ", 
       "-"}], " ", "..."}], " ", "-", " ", 
     RowBox[{
      RowBox[{"a", "[", "m", "]"}], 
      RowBox[{"X", "[", 
       RowBox[{
        RowBox[{"x", "[", 
         RowBox[{"[", "i", "]"}], "]"}], ",", "m"}], "]"}]}]}], 
    ")"}]}]}]], "DisplayFormula",
 Evaluatable->False],

Cell[TextData[{
 "Here the ",
 StyleBox["X", "MO"],
 StyleBox["s", "TR"],
 " are the basis functions and the ",
 StyleBox["effvar", "MO"],
 " is the effective variance error. In general, this is not linear in the \
parameters to which we are fitting. But in the case of a straight line, the \
derivative of the sum of the squares with respect to the intercept is linear, \
and we can set the derivative to zero."
}], "Text",
 Evaluatable->False,
 AspectRatioFixed->True,
 CellTags->{"LinearFit--Brent option", "Brent option to LinearFit"}],


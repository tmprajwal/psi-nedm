Cell["\<\
After folding in calibration and resolution numbers from the mass \
spectrometer, the two peaks can be approximated as Gaussians. The center of \
the first peak is 326.155 amu with standard deviation 0.0240 amu; the center \
of the second peak is 327.255 amu with a standard deviation of 0.0276 amu. \
These values are included in the following definition of the basis function.\
\>", "Text",
 Evaluatable->False,
 AspectRatioFixed->True,
 CellTags->{"Basis--option to LinearFit", "LinearFit--Basis option"}],

Cell[BoxData[
 RowBox[{
  RowBox[{"myBasis", "[", 
   RowBox[{"x_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"Module", "[", "\n", "\t\t", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"c1", "=", "326.155"}], ",", 
      RowBox[{"sigma1", "=", "0.0240"}], ",", 
      RowBox[{"c2", "=", "327.255"}], ",", 
      RowBox[{"sigma2", "=", "0.0276"}]}], "}"}], ",", "\n", "\t\t", 
    RowBox[{"Switch", "[", 
     RowBox[{"n", ",", "\n", "\t\t\t", "1", ",", 
      RowBox[{"N", "[", 
       RowBox[{"Exp", "[", 
        RowBox[{"-", 
         FractionBox[
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"x", "-", "c1"}], ")"}], "2"], 
          RowBox[{"2", " ", 
           SuperscriptBox["sigma1", "2"]}]]}], "]"}], "]"}], ",", "\n", 
      "\t\t\t", "2", ",", 
      RowBox[{"N", "[", 
       RowBox[{"Exp", "[", 
        RowBox[{"-", 
         FractionBox[
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"x", "-", "c2"}], ")"}], "2"], 
          RowBox[{"2", " ", 
           SuperscriptBox["sigma2", "2"]}]]}], "]"}], "]"}]}], "\n", "\t\t", 
     "]"}]}], "\n", "]"}]}]], "Input",
 AspectRatioFixed->True,
 CellTags->{"Basis--option to LinearFit", "LinearFit--Basis option"},
 CellLabel->"In[72]:="],

Cell[TextData[{
 "Note that the input arguments are the same as for ",
 StyleBox["Power", "MR"],
 ": the first is the value of the independent variable and the second is the \
factor. "
}], "Text",
 Evaluatable->False,
 AspectRatioFixed->True,
 CellTags->{"Basis--option to LinearFit", "LinearFit--Basis option"}],

Cell[TextData[{
 "We fit the ",
 StyleBox["MassSpecData", "MO"],
 StyleBox[".",
  FontSlant->"Italic"]
}], "Text",
 Evaluatable->False,
 AspectRatioFixed->True,
 CellTags->{"Basis--option to LinearFit", "LinearFit--Basis option"}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"result", "=", 
  RowBox[{"LinearFit", "[", 
   RowBox[{"MassSpecData", ",", 
    RowBox[{"{", 
     RowBox[{"1", ",", "2"}], "}"}], ",", "a", ",", 
    RowBox[{"Basis", "\[Rule]", "myBasis"}]}], "]"}]}]], "Input",
 AspectRatioFixed->True,
 CellTags->{"Basis--option to LinearFit", "LinearFit--Basis option"},
 CellLabel->"In[73]:="],


  FormBox[
   RowBox[{
    SuperscriptBox["area", 
     StyleBox[
      RowBox[{"1", " "}], "TR"]], 
    StyleBox["=", "TR"], " ", "area"}], TraditionalForm]],
  FontSlant->"Italic"],
 StyleBox[".",

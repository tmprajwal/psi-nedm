        ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{
          "#1", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], ",", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
             "#1", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
             "\[LessEqual]", "1"}], ",", "0.01", ",", "0.03"}], "]"}]}], 
         "}"}]}], "}"}], "&"}], ")"}], "/@", 
    RowBox[{"Flatten", "/@", "ThermocoupleData"}]}]}], ";"}]], "Input",
